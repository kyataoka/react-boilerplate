{
  "name": "react-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint ./src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint ./src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix --quiet && prettier --write ./src",
    "preview": "vite preview",
    "format": "prettier --write ./src",
    "gen:file:components": "hygen generator components",
    "gen:file:hooks": "hygen generator hooks",
    "gen:file:functions": "hygen generator functions",
    "gen:icon": "run-s gen:icon:icon gen:icon:light gen:icon:dark format:index",
    "gen:icon:icon": "pwa-asset-generator ./public/icon.svg ./public/icons --type png --favicon --index index.html --opaque false --icon-only; pwa-asset-generator ./public/icon.svg ./public/icons --type png --index index.html --icon-only",
    "gen:icon:light": "pwa-asset-generator ./public/icon.svg ./public/icons --type png --index index.html --splash-only --background \"#FFF\" --padding \"calc(50vh - 20%) calc(50vw - 40%)\"",
    "gen:icon:dark": "pwa-asset-generator ./public/icon.svg ./public/icons --type png --index index.html --splash-only --background \"#121212\" --dark-mode --padding \"calc(50vh - 20%) calc(50vw - 40%)\"",
    "format:index": "prettier --write ./index.html",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.13",
    "@mui/material": "^5.15.13",
    "@tanstack/react-query": "^5.28.8",
    "@tanstack/react-router": "^1.19.13",
    "axios": "^1.6.8",
    "clsx": "^2.1.1",
    "mui-nested-menu": "^3.3.0",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.2.25",
    "@storybook/addon-essentials": "^8.0.5",
    "@storybook/addon-interactions": "^8.0.5",
    "@storybook/addon-links": "^8.0.5",
    "@storybook/addon-onboarding": "^8.0.5",
    "@storybook/blocks": "^8.0.5",
    "@storybook/react": "^8.0.5",
    "@storybook/react-vite": "^8.0.5",
    "@storybook/test": "^8.0.5",
    "@tanstack/router-vite-plugin": "^1.19.6",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-functional": "^6.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prefer-arrow-functions": "^3.3.2",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "happy-dom": "^16.8.1",
    "hygen": "^6.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "pwa-asset-generator": "^6.3.1",
    "storybook": "^8.0.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-pwa": "^0.19.7",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^3.0.4"
  }
}