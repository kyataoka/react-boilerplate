{
  "name": "react-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint ./src --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint ./src --report-unused-disable-directives --max-warnings 0 --fix --quiet && prettier --write ./src",
    "preview": "vite preview",
    "format": "prettier --write ./src",
    "gen:file:components": "hygen generator components",
    "gen:file:hooks": "hygen generator hooks",
    "gen:file:functions": "hygen generator functions",
    "gen:icon": "run-s gen:icon:icon gen:icon:light gen:icon:dark format:index",
    "gen:icon:icon": "pwa-asset-generator ./public/icon.svg ./public/icons --type png --favicon --index index.html --opaque false --icon-only; pwa-asset-generator ./public/icon.svg ./public/icons --type png --index index.html --icon-only",
    "gen:icon:light": "pwa-asset-generator ./public/icon.svg ./public/icons --type png --index index.html --splash-only --background \"#FFF\" --padding \"calc(50vh - 20%) calc(50vw - 40%)\"",
    "gen:icon:dark": "pwa-asset-generator ./public/icon.svg ./public/icons --type png --index index.html --splash-only --background \"#121212\" --dark-mode --padding \"calc(50vh - 20%) calc(50vw - 40%)\"",
    "format:index": "prettier --write ./index.html",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "upgrade-packages": "npx npm-check-updates -u && pnpm install"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.2",
    "@mui/material": "^6.4.2",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-router": "^1.99.0",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "mui-nested-menu": "^4.0.1",
    "notistack": "^3.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/js": "^9.19.0",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-links": "^8.5.3",
    "@storybook/addon-onboarding": "^8.5.3",
    "@storybook/blocks": "^8.5.3",
    "@storybook/react": "^8.5.3",
    "@storybook/react-vite": "^8.5.3",
    "@storybook/test": "^8.5.3",
    "@tanstack/router-vite-plugin": "^1.99.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-functional": "^8.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prefer-arrow-functions": "^3.6.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-storybook": "^0.11.2",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "happy-dom": "^16.8.1",
    "hygen": "^6.2.11",
    "jiti": "^2.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "pwa-asset-generator": "^6.4.0",
    "storybook": "^8.5.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.0.11",
    "vite-plugin-pwa": "^0.21.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.4"
  }
}